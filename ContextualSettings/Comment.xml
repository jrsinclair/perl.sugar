<?xml version="1.0" encoding="UTF-8"?>

<settings>

	<setting name="comment-singleline-start">
		<syntax-context>perl, perl > *</syntax-context>
		<value>#</value>
	</setting>

	<setting name="comment-multiline-start">
		<syntax-context>perl, perl > *</syntax-context>
		<value>=</value>
	</setting>

	<setting name="comment-multiline-end">
		<syntax-context>perl, perl > *</syntax-context>
		<value>=cut</value>
	</setting>

	
</settings>
